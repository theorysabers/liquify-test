  function fadeVolume(to, duration = 300) {
    clearInterval(fadeInterval);
    const steps = 30;
    const stepTime = duration / steps;
    const start = audio.volume;
    const diff = to - start;
    let i = 0;
    fadeInterval = setInterval(() => {
      i++;
      audio.volume = Math.min(Math.max(start + (diff * (i / steps)), 0), 1);
      if (i >= steps) clearInterval(fadeInterval);
    }, stepTime);
  }
  hoverTargets.forEach(target => {
    target.addEventListener('mouseenter', () => fadeVolume(0.2));
    target.addEventListener('mouseleave', () => fadeVolume(0));
  });
</script>
